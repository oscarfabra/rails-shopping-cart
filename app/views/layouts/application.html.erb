<!DOCTYPE html>
<html>
  <head>
    <title>Rails Shopping Cart</title>
    <%= favicon_link_tag '/favicon.ico' %>
    <%= stylesheet_link_tag    'application', media: 'all', 
    'data-turbolinks-track' => true %>
    <%= stylesheet_link_tag "https://fonts.googleapis.com/css?family=Pacifico", 
      media: 'all', type: 'text/css' %>
    <%= stylesheet_link_tag "https://fonts.googleapis.com/css?family=Raleway:400,700",
      media: 'all', type: 'text/css' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
    <%= csrf_meta_tags %>
    <%= render 'layouts/shim' %>
  </head>
  <body>
    <%= render 'layouts/header' %>
    <div class="container">
      <p>This is not the static pages layout.</p>
      <%= yield %>
      <%= render 'layouts/footer' %>
      <!-- <%= debug(params) if Rails.env.development? %> -->
    </div>
  </body>
</html>
